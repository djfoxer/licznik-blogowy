{
  "name": "Licznik Blogowy dobreprogramy",
  "version": "1.3",
  "description": "Wtyczka przedstawia szczegółowe statystyki związane z Twoimi wpisami na blogu portalu dobreprogramy.pl",
  "manifest_version": 2,
  "browser_action": {
	  "default_title": "Licznik Blogowy dobreprogramy" 
  },
  "icons": { 
			"16": "icon16.png",
			"48": "icon48.png",
			"128": "icon128.png" },
  "content_scripts": [
    {
      "matches": ["https://www.dobreprogramy.pl/*"],
      "js": ["jquery.min.js","awesomechart.js", "b.js"]
    }
  ],
  "permissions": [
	  "https://www.dobreprogramy.pl/*",
	  "http://dp.do/*"
	],
  "homepage_url": "https://www.dobreprogramy.pl/djfoxer"
}